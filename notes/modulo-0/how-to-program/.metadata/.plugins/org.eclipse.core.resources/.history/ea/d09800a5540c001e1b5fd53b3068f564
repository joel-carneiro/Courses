package ifce;

import java.util.Scanner;

public class RegisterStudent {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.print("NOME COMPLETO: ");
		input.nextLine();
		
		System.out.print("CODE: ");
		input.nextLine();
		
		System.out.print("CURSO: ");
		input.nextLine();
		
	}
}

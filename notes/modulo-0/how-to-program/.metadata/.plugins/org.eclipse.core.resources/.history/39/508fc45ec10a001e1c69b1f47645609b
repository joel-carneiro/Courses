package welcome;

import java.util.Scanner;

public class School {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		System.out.print("Média do aluno: ");
		final double MEAN = scanner.nextDouble();
		
		String result = MEAN >= 77 ? "APROVADO" : "REPROVADO";
		
		System.out.println("SITUAÇÃO: " + result);
	}
}
